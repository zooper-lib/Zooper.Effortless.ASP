<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Version>2.0.0</Version>
        <PackageVersion>$(Version)</PackageVersion>
        <AssemblyVersion>$(Version).0</AssemblyVersion>
        <FileVersion>$(Version).0</FileVersion>
    </PropertyGroup>

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="JetBrains.Annotations" Version="2024.2.*"/>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="8.0.*"/>
        <PackageReference Include="Microsoft.OpenApi" Version="1.6.*"/>
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.7.*"/>
        <PackageReference Include="Swashbuckle.AspNetCore.Filters" Version="8.0.*"/>
    </ItemGroup>

</Project>
